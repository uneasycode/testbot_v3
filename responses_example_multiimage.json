# responses_example.json - Example responses.json with multiple-images support

{
    "hello": {
        "type": "text",
        "content": [
            "Hi there!",
            "Hello! How can I help?",
            "Hey, what's up?"
        ]
    },
    "bye": {
        "type": "text",
        "content": [
            "Goodbye!",
            "See you later!",
            "Take care!"
        ]
    },
    "show photo": {
        "type": "image",
        "content": "single_photo.jpg",
        "caption": "Here's the photo you requested!"
    },

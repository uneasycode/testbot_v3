        "type": "multiple-images",
        "content": [
            "photo1.jpg",
            "photo2.jpg", 
            "photo3.jpg",
            "photo4.jpg"
        ],
        "caption": "Here's our photo gallery!"
    },
    "portfolio": {
        "type": "multiple-images",
        "content": [
            "design1.png",
            "design2.png",
            "design3.png"
        ],
        "caption": "Check out my design portfolio"
    },
    "play music": {
        "type": "audio",
        "content": "song.mp3"
    },
    "aap hello bol sakte ho": {
        "type": "text",
        "content": [
            "haan"
        ]
    },
    "khana khaya": {
        "type": "text",
        "content": [
            "ji kha liya"
        ]
    },
    "product images": {
        "type": "multiple-images",
        "content": [
            "product_front.jpg",
            "product_back.jpg",
            "product_side.jpg",
            "product_detail.jpg"
